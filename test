function getSmallestMultiple(){
    var i = 20;
    do{
        if(isDivisibleByAll(i)){
            return i;
        }
            
        i += 20;        

    }while(1);
}

function isDivisibleByAll(num){
    for(var i = 20; i >= 2 ; i--){
        if(num % i != 0)
            return false;
    }
    return true;
}

var divisors = '1,2,3';
divisors = divisors.split();
divisorsforEach(function (num){
    console.log(num)
});
